  <Target Name="PrepareStaticContent" DependsOnTargets="BuildAjaxMinManifests" Condition="'$(NCrunch)' != '1'">
    <!-- Delete existing static folder for current version. -->
    <RemoveDir Directories="$(ProjectDir)obj\Static" />
    <!-- (Re-)create static folder. -->
    <!-- <MakeDir Directories="$(ProjectDir)obj\Bundles;$(ProjectDir)obj\Static\Images;$(ProjectDir)obj\Static\Scripts;$(ProjectDir)obj\Static\Styles" /> -->
    <MakeDir Directories="$(ProjectDir)obj\Static;$(ProjectDir)obj\Static\Scripts;$(ProjectDir)obj\Static\Styles" />
    <!-- Copy static assets into static directory. -->
    <!--<Exec Command="xcopy &quot;$(ProjectDir)Images\*.*&quot; &quot;$(ProjectDir)obj\Static\Images&quot; /y" />-->
    <Exec Command="xcopy &quot;$(ProjectDir)Styles\*.min.css&quot; &quot;$(ProjectDir)obj\Static\Styles&quot; /y" />
    <Exec Command="xcopy &quot;$(ProjectDir)Scripts\*.min.js&quot; &quot;$(ProjectDir)obj\Static\Scripts&quot; /y" />
    <!-- Move files to filename that contains hash. -->
    <ItemGroup>
      <!--<StaticFilesForHashing Include="$(ProjectDir)obj\Static\Images\**\*" />-->
      <StaticFilesForHashing Include="$(ProjectDir)obj\Static\Scripts\**\*" />
      <StaticFilesForHashing Include="$(ProjectDir)obj\Static\Styles\**\*" />
    </ItemGroup>
    <RenameWithHash SourceFiles="@(StaticFilesForHashing)" />
  </Target>
  <PropertyGroup>
